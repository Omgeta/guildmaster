[sub_resource type="Resource" id="Resource_q6hg5"]
script = ExtResource("1_q6hg5")
text = "You wake up in a desolate land, with a tower in front of you.
Your instincts scream at you."
auto_advance = false
hold_time = 1.5
metadata/_custom_type_script = "uid://88114vdfe28f"

[sub_resource type="Resource" id="Resource_pbgfw"]
script = ExtResource("1_q6hg5")
text = "You must climb, or you won't be able to return to your world."
auto_advance = false
hold_time = 1.5
metadata/_custom_type_script = "uid://88114vdfe28f"

[sub_resource type="Resource" id="Resource_g2ihi"]
script = ExtResource("1_q6hg5")
text = "Suddenly, a light appears from behind you..."
auto_advance = false
hold_time = 1.5
metadata/_custom_type_script = "uid://88114vdfe28f"

[sub_resource type="Resource" id="Resource_g1i3c"]
script = ExtResource("1_q6hg5")
text = "You turn to see the light take shape into a human…"
auto_advance = false
hold_time = 1.5
metadata/_custom_type_script = "uid://88114vdfe28f"

[sub_resource type="Resource" id="Resource_6yjwu"]
script = ExtResource("1_q6hg5")
text = "\"Welcome, Seeker,\" they say. \"You are not the first, and you won't be the last."
auto_advance = false
hold_time = 1.5
metadata/_custom_type_script = "uid://88114vdfe28f"

[sub_resource type="Resource" id="Resource_tlann"]
script = ExtResource("1_q6hg5")
text = "'\"The tower grants power - but it demands something in return.\""
auto_advance = false
hold_time = 1.5
metadata/_custom_type_script = "uid://88114vdfe28f"

[sub_resource type="Resource" id="Resource_85rst"]
script = ExtResource("1_q6hg5")
text = "You ask, 'What must I give up?'"
auto_advance = false
hold_time = 1.5
metadata/_custom_type_script = "uid://88114vdfe28f"

[sub_resource type="Resource" id="Resource_xiwm5"]
script = ExtResource("1_q6hg5")
text = "The figure smiles. \"Everything you’re unwilling to lose.\""
auto_advance = false
hold_time = 1.5
metadata/_custom_type_script = "uid://88114vdfe28f"

[sub_resource type="Resource" id="Resource_8oiy1"]
script = ExtResource("1_q6hg5")
text = "\"But fear not. Allies await. The guild will guide you.\""
auto_advance = false
hold_time = 1.5
metadata/_custom_type_script = "uid://88114vdfe28f"

[sub_resource type="Resource" id="Resource_tk4y7"]
script = ExtResource("1_q6hg5")
text = "As they vanish, a sigil glows before you - a gateway to the first floor of the tower…"
auto_advance = false
hold_time = 1.5
metadata/_custom_type_script = "uid://88114vdfe28f"

